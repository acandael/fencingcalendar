<%= simple_form_for([:admin, tournament]) do |f| %>
  <%= f.input :title %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.association :country %>
  <%= f.input :location %>
  <%= f.association :category %>
  <%= f.association :weapons, as: :check_boxes %>
  <%= f.association :organization %>
  <%= f.input :link %>
  <%= f.button :submit %>
<% end %>
